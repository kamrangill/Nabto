<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Nabto.Views.Tunneling"
             Title="Nabto"
             >
  
  <StackLayout VerticalOptions="StartAndExpand" Padding="5">
    <Label  x:Name="txtState"  VerticalOptions="Center" HorizontalOptions="Center" />

  
    <Button Text="Create Tunnel"  Clicked="TunnelingClicked"/>
    <Button Text="Get Tunnel Status"  Clicked="GetTunnelStatusClicked"/>
   <Button Text="Shutdown Tunnel"  Clicked="TunnelShutdownClicked"/>
  </StackLayout>
</ContentPage>